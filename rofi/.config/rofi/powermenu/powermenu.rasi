@import "/usr/share/rofi/themes/gruvbox-dark-soft.rasi"

* {
scrollbar: false;
}

window {
  width:    300px;
  border:   0;
  background-color: #202020;
  location: northeast;
  x-offset: -10px;
  y-offset: 35px;
  padding:  10px;
  children: [ listview ];
}


listview {
  border: 0;
  layout:   vertical;
  lines:    5;
  columns:  1;
}

element {
  padding:  5px 5px;
}
